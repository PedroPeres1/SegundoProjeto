package com.pedro;

import java.io.IOException;
import java.util.ArrayList;
import java.util.List;

import javax.servlet.ServletConfig;
import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

/**
 * Servlet implementation class ListaCachorro
 */
@WebServlet("/ListaCachorro")
public class ListaCachorro extends HttpServlet {

	/**
	 * Default constructor.
	 */
	public ListaCachorro() {
		super();
		// TODO Auto-generated constructor stub
	}

	protected void doGet(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException {
		// TODO Auto-generated method stub
		
		response.getWriter().println("<html>");
		
		response.getWriter().println("<head>");
		response.getWriter().println("<h1><title>Lista dos Cachorros</title></h1>");
		response.getWriter().println("</head>");
			 
		response.getWriter().println("<body>");
		response.getWriter().println("<ul>");
		response.getWriter().println("<li>Nick</li>");
		response.getWriter().println("<li>Fred</li>");
		response.getWriter().println("<li>Lili</li>");
		response.getWriter().println("<li>Mel</li>");
		response.getWriter().println("</ul>");
		response.getWriter().println("</body>");
		
	  response.getWriter().println("</html>");
		

	}

}
